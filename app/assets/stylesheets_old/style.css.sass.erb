#page
  margin: 0 auto
  margin-bottom: 0px
  margin-left: auto
  margin-right: auto
  margin-top: 0px
  width: 1000px

#header
  background-attachment: initial
  background-clip: initial
  background-image: url(<%= image_path ( "logo.png" ) %>) no-repeat 17px 6px
  background-origin: initial
  background-position-x: 50%
  background-position-y: 0%
  background-repeat-x: repeat
  background-repeat-y: no-repeat
  height: 57px

#main
  background-attachment: initial
  background-clip: initial
  background-color: white
  background-origin: initial
  background-position-x: 100%
  background-position-y: 50%
  background-repeat-x: no-repeat
  background-repeat-y: repeat
  height: 1%
  padding: 0 0 15px
  padding-bottom: 15px
  padding-left: 0px
  padding-right: 0px
  padding-top: 0px

#left_col
  background-attachment: initial
  background-clip: initial
  background-color: initial
  background-origin: initial
  background-position-x: 50%
  background-position-y: 0%
  background-repeat-x: repeat
  background-repeat-y: no-repeat
  float: left
  font-size: 13px
  padding: 35px 20px 10px 20px
  padding-bottom: 10px
  padding-left: 20px
  padding-right: 20px
  padding-top: 35px
  width: 630px

#right_col
  float: right
  width: 327px

#subFooter
  background: #101010
  background-attachment: initial
  background-clip: initial
  background-color: #101010
  background-image: initial
  background-origin: initial
  background-position-x: initial
  background-position-y: initial
  background-repeat-x: initial
  background-repeat-y: initial
  clear: both
  color: #606060
  font-size: 13px
  line-height: 1.6em
  padding: 35px 25px
  padding-bottom: 35px
  padding-left: 25px
  padding-right: 25px
  padding-top: 35px

#footer
  background: black
  background-attachment: initial
  background-clip: initial
  background-color: black
  background-image: initial
  background-origin: initial
  background-position-x: initial
  background-position-y: initial
  background-repeat-x: initial
  background-repeat-y: initial
  clear: both
  color: #4A4A4A
  font-size: 13px
  margin-bottom: 26px
  padding: 25px
  padding-bottom: 25px
  padding-left: 25px
  padding-right: 25px
  padding-top: 25px

body
  background-image: url(<%= image_path ( "lghtmesh.png" ) %>)
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif
  font-size: 13px
  text-align: center

a
  color: #D6AC04
  &:active, &:visited
    color: #D6AC04
h1
  margin-bottom: 5px
  font-family: 'Droid Sans', sans-serif

h2, h3
  font-family: 'Droid Sans', sans-serif

h1
  display: block
  font-size: 2em
  font-weight: bold

h2
  font-size: 150%
  font-style: normal
  font-variant: normal
  font-weight: bold
  font-weight: normal
  line-height: normal
  margin-bottom: 0
  padding-bottom: 0
  webkit-margin-after: 0.83em
  webkit-margin-before: 0.83em
  webkit-margin-end: 0px
  webkit-margin-start: 0px

#container
  background-color: #EEE
  border-left: 1px solid #DCCBAF
  border-right: 1px solid #DCCBAF
  border-top: 1px solid #DCCBAF
  margin-left: auto
  margin-right: auto
  text-align: left
  width: 75%

#inner-content
  background-color: #EEEEEE
  float: left
  height: 100%
  width: 74%
  padding-left: 5px

#header
  background-color: white
  height: 100px
  margin-top: 5px
  padding-left: 5px
  padding-right: 5px
  padding-top: 5px

#logo
  background-image: url(<%= image_path ( "logo_new.png" ) %>)
  background-repeat: no-repeat
  float: left
  height: 90px
  width: 50%

#search
  float: right
  height: 30px
  text-align: right
  width: 50%

#menu
  height: 20px
  text-align: right

#content
  background-color: #EEE
  width: 100%

#divider
  background-color: black
  height: 20px

#side-bar
  background-color: #e6f0d8
  border-bottom: 1px solid #DCCBAF
  border-left: 1px solid #DCCBAF
  float: right
  width: 25%
  padding-left: 5px

#footer
  background-color: #494442
  border-top: 1px solid #DCCBAF
  clear: both
  height: 100px
  text-align: center
  color: white

.yui-skin-sam td.column-wrap
  word-wrap: break-word

#menu
  background-color: #333
  height: 45px
  width: 100%
  ul
    float: left
    margin: 0
    padding: 0
    li
      display: inline
      a
        background-color: #333
        color: white
        float: left
        padding: 10.5px 11px
        text-decoration: none
        &:visited
          color: white
        &:hover
          background-color: #0b75b2
          color: #fff

.menu .current
  background-color: #0b75b2
  color: #fff

p
  padding-bottom: 1em
  word-wrap: break-word

strong, th
  font-weight: bold

table.tablesorter
  background-color: #CDCDCD
  font-family: arial
  font-size: 8pt
  margin: 10px 0pt 15px
  text-align: left
  width: 100%
  thead th, tfoot th
    background-color: #e6EEEE
    border: 1px solid #FFF
    font-size: 8pt
    padding: 4px
  thead .header
    background-image: url(<%= image_path ( "bg.gif" ) %>)
    background-position: center right
    background-repeat: no-repeat
    cursor: pointer
  tbody
    td
      background-color: #FFF
      color: #3D3D3D
      padding: 4px
      vertical-align: top
    tr.odd td
      background-color: #F0F0F6
  thead tr
    .headerSortUp
      background-image: url(<%= image_path ( "asc.gif" ) %>)
    .headerSortDown
      background-image: url(<%= image_path ( "desc.gif" ) %>)
      background-color: #8dbdd8
    .headerSortUp
      background-color: #8dbdd8

dl
  display: block
  webkit-margin-after: 1em
  webkit-margin-before: 1em
  webkit-margin-end: 0px
  webkit-margin-start: 0px

dt
  clear: left
  display: block
  float: left
  font-weight: bold
  margin-bottom: 5px
  text-align: middle
  width: 200px

dd
  display: block
  float: left
  margin-bottom: 5px
  webkit-margin-start: 40px
  width: 500px

#login
  float: right
  height: 60%

.list-style
  list-style-type: none
  margin: 0
  padding: 0

#bill
  background: #FFD
  border: 1px solid #cccccc
  margin: 20pt 10pt
  padding: 10pt
  u
    background-color: yellow
  strike
    color: red

#inner-side-bar
  padding-bottom: 10px
  padding-top: 3px
  width: 100%
  word-wrap: break-word

.details-image
  background-image: url(<%= image_path ( "detals.png" ) %>)
  background-repeat: no-repeat
  float: left
  height: 50px
  width: 50px

#status-bar
  font-size: 11px
  ul
    color: darkgray
    list-style-type: none
    margin: 0
    padding: 0
    li
      border-bottom: 1px solid #DCCBAF
      border-left: 1px solid #DCCBAF
      border-right: 1px solid #DCCBAF
      border-top: 1px solid #DCCBAF
      display: inline
      padding: .2em 1em

.mark-status
  background-color: green
  color: white
  font-weight: bold

.mark-status-failed
  background-color: red
  color: white
  font-weight: bold

#side-bar h4
  border-bottom: 1px solid black
  font-size: 13px
  font-weight: bold
  margin-bottom: 0.5em
  width: 95%

#qr-image
  font-size: 13px
  text-align: center

#jstwitter
  color: #333333
  font-family: georgia
  font-size: 13px
  padding: 10px
  text-wrap: suppress
  .tweet
    border-bottom: 1px dotted #ccc
    margin: 0 auto 15px auto
    padding: 0 0 15px 0
    a
      color: #13c9d0
      text-decoration: none
      &:hover
        text-decoration: underline
    .time
      color: #666666
      font-size: 10px
      font-style: italic

.warning-text
  color: red

title
  display: block
